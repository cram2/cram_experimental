<package format="1">
  <name>cram_projection_demos</name>
  <version>0.2.1</version>
  <description>A metapackage for packages with demos using CRAM in projection</description>
  <author>Gayane Kazhoyan</author>
  <maintainer email="kazhoyan@cs.uni-bremen.de">Gayane Kazhoyan</maintainer>
  <license>BSD</license>

  <url type="website">http://cram-system.org</url>
  <url type="repository">http://github.com/gaya-/cram_projection_demos</url>

  <buildtool_depend>catkin</buildtool_depend>

  <run_depend>spatial_relations_demo</run_depend>

  <!-- to be able to run the launch files of the single child packages -->
  <run_depend>navigation</run_depend>
  <run_depend>pr2_description</run_depend>
  <run_depend>moveit_full</run_depend>
  <run_depend>pr2_moveit_config</run_depend>
  <run_depend>map_server</run_depend>
  <export>
    <metapackage/>
  </export>
</package>

